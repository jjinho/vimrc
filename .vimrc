execute pathogen#infect()
syntax enable
set ruler
set tabstop=4
set softtabstop=4
set shiftwidth=4
set expandtab
set number
set showcmd
"set cursorline
filetype indent on
set wildmenu
set lazyredraw
set colorcolumn=80
"highlight ColorColumn ctermbg=0
" Color scheme
set background=dark
let g:solarized_termcolors=256
colorscheme solarized
